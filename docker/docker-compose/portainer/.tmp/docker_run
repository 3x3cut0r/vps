docker container run -d --restart=always \
    --name=portainer-temp \
    --network=portainer \
    --ip=10.24.1.3 \
    --dns=8.8.8.8 \
    -p 9101:9000 \
    -e COMPOSE_HTTP_TIMEOUT=240 \
    -v /run/user/2000/docker.sock:/var/run/docker.sock \
    -v portainer-data:/data \
    portainer/portainer-ce:alpine
